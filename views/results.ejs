<h1>results page.</h1>

<div style="display: flex">
    <% data["Search"].forEach(function(movie){ %>
    <div style="display: inline">
        <img src="<%= movie['Poster'] %>" alt="">
        <figcaption style="display: block">
            <strong><%= movie["Title"] %></strong> - <%= movie["Year"] %>
        </figcaption>
    </div>
            <% }) %>

</div>

